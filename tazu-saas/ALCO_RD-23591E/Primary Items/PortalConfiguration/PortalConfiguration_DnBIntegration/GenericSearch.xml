<?xml version="1.0" encoding="UTF-8"?>
<screen id="GenericSearch" type="GenericSearch">
      <parameter id="AutoForwardSingleResultEnabled" value="true"/>
      <component id="NodeList" type="NodeList">
        <parameter id="LookupScreenTypeForNavigation" value="true"/>
        <parameter id="PageSize" value="50"/>
        <parameter id="DefaultSortingOrder" value="Ascending"/>
        <parameter id="EmptySelectionScreen" value="EmptySelectionScreen"/>
        <parameter id="EnableDefaultSorting" value="true"/>
        <parameter id="ClearSelection" value="Clear selection"/>
        <parameter id="MultiSelectionScreen" value="MultiSelectionScreen"/>
        <parameter id="ProductSelectionScreen" value="Forwarding Switch Screen"/>
        <parameter id="ClearAllFilters" value="Clear all filters"/>
        <parameter id="ToolbarHeight" value="30"/>
        <parameter id="ClassificationSelectionScreen" value="Classification Screen"/>
        <parameter id="HideStandardButtons" value="false"/>
        <parameter id="UseDetailsOverlay" value="true"/>
        <parameter id="ID" value="ClassificationNodeList"/>
        <parameter id="AssetSelectionScreen" value="assetdetails"/>
        <parameter id="SelectAllLabel" value="Select all"/>
        <parameter id="EntitySelectionScreen" value="Forwarding Switch Screen"/>
        <parameter-list id="Actions">
          <component type="AddToBasketAction">
            <parameter id="Message" value="Object(s) added to basket"/>
            <parameter id="Label" value="Add to basket"/>
            <parameter id="IconStyleClass" value="stibo-AssetPort-Toolbar-Basket-add"/>
          </component>
          <component type="DownloadAssetsAction">
            <parameter id="CancelDownloadButtonText" value="Cancel"/>
            <parameter id="Label" value="Download assets"/>
            <parameter id="PipelineSelectionText" value="Select Image conversion"/>
            <parameter id="NoContentWarning" value="Collection contains asset(s) with no content in context"/>
            <parameter id="IconStyleClass" value="DownloadAssetsButton"/>
            <parameter id="NoSelectionMessage" value="No assets selected"/>
            <parameter id="StartDownloadButtonText" value="Start download"/>
            <parameter id="Title" value="Download assets"/>
          </component>
          <component type="ExportAction">
            <parameter id="SelectionHeader" value="Choose Export Configuration"/>
            <parameter id="CancelDownloadButtonText" value="Cancel export"/>
            <parameter id="NoExportConfigurationsMessage" value="No export configurations found"/>
            <parameter id="IconStyleClass" value="stibo-AssetPort-Toolbar-ActiveIcon-download"/>
            <parameter id="NoSelectionMessage" value="No nodes selected"/>
            <parameter id="ActionTitle" value="Export to Excel"/>
            <parameter id="DialogTitle" value="Export to Excel"/>
            <parameter id="StartDownloadButtonText" value="Start export"/>
            <parameter-list id="NarrowedExportConfigurations"/>
          </component>
          <component type="CreateCollectionAction">
            <parameter id="ButtonLabel" value="i18n.stibo.Collections.CreateCollection.Label"/>
            <component id="CreateCollectionDialog" type="CreateCollectionDialog">
              <component id="NodePickerDialog" type="NodePickerDialog">
                <parameter id="Title" value="i18n.stibo.NodePickerDialog.Title"/>
                <component id="RootNodePlugin" type="StaticRootNodes">
                  <parameter-list id="RootNodeURLs">
                    <parameter value="step://collectiongroup?id=CollectionGroup+root"/>
                  </parameter-list>
                </component>
                <parameter-list id="TabPages">
                  <component type="BrowseTab"/>
                </parameter-list>
              </component>
            </component>
          </component>
        </parameter-list>
        <parameter-list id="DisplayModes">
          <component type="TableDisplayMode">
            <parameter id="EnableFiltering" value="true"/>
            <parameter id="ContextHelp" value="List view"/>
            <parameter id="ShowDetails" value="false"/>
            <parameter id="DisplayModeHeight" value="200"/>
            <parameter id="ColumnsResizable" value="false"/>
            <parameter-list id="Headers">
              <component type="IDHeader">
                <parameter id="Label" value="ID"/>
                <parameter id="EnableLink" value="true"/>
              </component>
              <component type="TitleHeader">
                <parameter id="Label" value="Name"/>
              </component>
              <component type="ObjectTypeHeader">
                <parameter id="Label" value="Type"/>
              </component>
              <component type="ValueHeader">
                <parameter id="ShowLOVIDs" value="false"/>
                <parameter id="Mandatory" value="NotMandatory"/>
                <parameter id="Attribute" value="asset.extension"/>
                <parameter id="Label" value="File extension"/>
                <parameter id="ShowInvalidInheritedValues" value="false"/>
                <parameter id="nowrap" value="false"/>
                <parameter id="Readonly" value="false"/>
              </component>
              <component type="ValueHeader">
                <parameter id="ShowLOVIDs" value="false"/>
                <parameter id="Mandatory" value="NotMandatory"/>
                <parameter id="Attribute" value="Description"/>
                <parameter id="ShowInvalidInheritedValues" value="false"/>
                <parameter id="nowrap" value="false"/>
                <parameter id="Readonly" value="false"/>
              </component>
              <component type="ThumbnailHeader">
                <parameter id="Label" value="Thumb"/>
              </component>
            </parameter-list>
          </component>
          <component type="FilmStripDisplayMode">
            <parameter id="ContextHelp" value="Filmstrip view"/>
            <parameter id="ShowDetails" value="true"/>
            <parameter id="DisplayModeHeight" value="200"/>
          </component>
          <component type="GalleryDisplayMode">
            <parameter id="ContextHelp" value="Gallery view"/>
            <parameter id="ShowDetails" value="false"/>
            <parameter id="DisplayModeHeight" value="200"/>
          </component>
          <component type="MultiEditDisplayMode">
            <parameter id="ShowEmptyGroupHeaders" value="true"/>
            <parameter id="EnableFiltering" value="true"/>
            <parameter id="ContextHelp" value="Multi edit view"/>
            <parameter id="ShowDetails" value="false"/>
            <parameter id="ShowGroupHeaders" value="true"/>
            <parameter id="DisplayModeHeight" value="200"/>
            <parameter id="GroupsInitiallyOpen" value="false"/>
            <parameter id="ShowOnlyValidAttributes" value="true"/>
            <parameter id="UseImmediateSave" value="false"/>
            <parameter id="PropertyDirection" value="HORIZONTAL"/>
            <parameter-list id="Headers">
              <component type="ThumbnailHeader"/>
              <component type="IDHeader">
                <parameter id="EnableLink" value="true"/>
              </component>
              <component type="TitleHeader"/>
            </parameter-list>
          </component>
        </parameter-list>
      </component>
    </screen>
