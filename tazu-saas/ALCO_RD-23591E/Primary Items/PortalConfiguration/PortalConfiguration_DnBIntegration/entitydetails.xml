<?xml version="1.0" encoding="UTF-8"?>
<screen id="entitydetails" type="NodeDetails">
      <parameter id="Title" value="i18n.stibo.NodeDetailsServerComponent.Title"/>
      <parameter id="EnforceValidity" value="false"/>
      <parameter id="ShowWarning" value="false"/>
      <component id="Main" type="NodeEditor">
        <parameter id="LabelLayout" value="LEFT"/>
        <parameter-list id="Rows">
          <component id="Rows" type="ValueGroup">
            <parameter id="EnableTagConversion" value="true"/>
            <parameter id="ReadOnly" value="false"/>
            <parameter id="EnableStepTags" value="false"/>
            <parameter id="DisplayValueHistory" value="true"/>
            <parameter id="SortLOVByID" value="false"/>
            <parameter id="AttributeGroup" value="DnBMatchingAttributes"/>
            <parameter id="RichTextEditing" value="Never"/>
            <parameter id="DisplayContextHelp" value="true"/>
            <parameter id="DisplayMode" value="Merged"/>
            <parameter id="MaxSuggestions" value="10"/>
            <parameter id="ShowLOVIDs" value="false"/>
            <parameter id="EnableLocaleFormatting" value="false"/>
            <parameter id="IncludeReferences" value="true"/>
            <parameter id="SuggestOracle" value="false"/>
            <parameter id="FilterAttributeType" value="Both"/>
          </component>
        </parameter-list>
      </component>
      <component id="Buttons" type="Buttons">
        <parameter-list id="Actions">
          <component id="Actions" type="StartWorkflowAction">
            <parameter id="ConfirmationMessage" value="Customer initiated in Matching workflow"/>
            <parameter id="ButtonLabel" value="Start DnB Matching"/>
            <parameter id="Workflow" value="DnBMatch Candidate"/>
            <parameter id="ButtonType" value="ICON_AND_TEXT"/>
          </component>
          <component id="Actions" type="StartWorkflowAction">
            <parameter id="ConfirmationMessage" value="Customer initiated in Profile workflow"/>
            <parameter id="ButtonLabel" value="Start DnB Profiling"/>
            <parameter id="Workflow" value="DnBCompanyProfile"/>
            <parameter id="ButtonType" value="ICON_AND_TEXT"/>
          </component>
        </parameter-list>
      </component>
    </screen>
