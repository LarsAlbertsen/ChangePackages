          </parameter-list>
        </component>
        <component id="Mappings" type="ScreenMapping">
          <parameter id="Screen" value="productdetails"/>
          <parameter-list id="Conditions">
            <component id="Conditions" type="ProductCondition"/>
          </parameter-list>
        </component>
        <component id="Mappings" type="ScreenMapping">
          <parameter id="Screen" value="assetdetails"/>
          <parameter-list id="Conditions">
            <component id="Conditions" type="AssetCondition"/>
          </parameter-list>
        </component>
        <component id="Mappings" type="ScreenMapping">
          <parameter id="Screen" value="BackgroundProcessesList"/>
          <parameter-list id="Conditions">
            <component id="Conditions" type="BackgroundProcessListCondition"/>
          </parameter-list>
        </component>
        <component id="Mappings" type="ScreenMapping">
          <parameter id="Screen" value="RecycleBin"/>
          <parameter-list id="Conditions">
            <component id="Conditions" type="RecycleBinCondition"/>
          </parameter-list>
        </component>
        <component id="Mappings" type="ScreenMapping">
          <parameter id="Screen" value="CollectionContent"/>
          <parameter-list id="Conditions">
            <component id="Conditions" type="CollectionCondition"/>
          </parameter-list>
        </component>
        <component id="Mappings" type="ScreenMapping">
          <parameter id="Screen" value="CollectionsList"/>
          <parameter-list id="Conditions">
            <component id="Conditions" type="CollectionGroupCondition"/>
          </parameter-list>
        </component>
        <component id="Mappings" type="ScreenMapping">
          <parameter id="Screen" value="AMS"/>
          <parameter-list id="Conditions">
            <component id="Conditions" type="ObjectTypeCondition">
              <parameter id="ObjectType" value="stibo.normalattribute"/>
            </component>
          </parameter-list>
        </component>
        <component id="Mappings" type="ScreenMapping">
          <parameter id="Screen" value="AGMS"/>
          <parameter-list id="Conditions">
            <component id="Conditions" type="ObjectTypeCondition">
              <parameter id="ObjectType" value="stibo.grouping attribute"/>
            </component>
          </parameter-list>
        </component>
        <component id="Mappings" type="ScreenMapping">
          <parameter id="Screen" value="D&amp;B Match Candidates ScrV2"/>
          <parameter-list id="Conditions">
            <component id="Conditions" type="WorkflowCondition">
              <parameter id="State" value="SelectCandidate"/>
              <parameter id="Workflow" value="DnBMatch"/>
            </component>
          </parameter-list>
        </component>
      </parameter-list>
      <parameter-list id="Preferences">
        <component id="Preferences" type="ScreenPreference">
          <parameter id="SidePanel" value="SHOWN"/>
          <parameter id="Screen" value="productfolderdetails"/>
        </component>
        <component id="Preferences" type="ScreenPreference">
          <parameter id="SidePanel" value="SHOWN"/>
          <parameter id="Screen" value="Blank"/>
        </component>
        <component id="Preferences" type="ScreenPreference">
          <parameter id="SidePanel" value="SHOWN"/>
          <parameter id="Screen" value="productdetails"/>
        </component>
      </parameter-list>
