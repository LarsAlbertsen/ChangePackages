            <parameter id="ButtonType" value="ICON_AND_TEXT"/>
            <parameter id="CssClass" value="DeleteButton"/>
          </component>
          <component id="Actions" type="ProofViewAction">
            <parameter id="OutputXmlValues" value="false"/>
