          </component>
          <component id="TabPages" type="SubScreenTabPage">
            <parameter id="Lazy" value="false"/>
            <parameter id="Title" value="Multi Context"/>
            <parameter id="DetailsScreen" value="MultiContextScreen"/>
          </component>
          <component id="TabPages" type="SubScreenTabPage">
            <parameter id="Lazy" value="false"/>
            <parameter id="Title" value="Children"/>
            <parameter id="DetailsScreen" value="ChildrenScreen"/>
          </component>
        </parameter-list>
      </component>
      <component id="Buttons" type="Buttons">
        <parameter-list id="Actions">
          <component id="Actions" type="SaveAction">
            <parameter id="ButtonLabel" value="Save"/>
            <parameter id="EnforceValidity" value="false"/>
            <parameter id="ButtonType" value="ICON_AND_TEXT"/>
            <parameter id="CssClass" value="SaveButton"/>
            <parameter id="ReloadAfterSave" value="true"/>
          </component>
          <component id="Actions" type="ResetAction">
            <parameter id="ButtonLabel" value="Reset"/>
            <parameter id="ButtonType" value="ICON_AND_TEXT"/>
            <parameter id="CssClass" value="ResetButton"/>
          </component>
          <component id="Actions" type="ApproveAction">
            <parameter id="ButtonLabel" value="Approve"/>
            <parameter id="EnforceValidity" value="false"/>
            <parameter id="ButtonType" value="ICON_AND_TEXT"/>
