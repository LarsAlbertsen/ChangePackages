<?xml version="1.0" encoding="UTF-8"?>
<screen id="productdetails" type="NodeDetails">
      <parameter id="Title" value="Product Details"/>
      <parameter id="EnforceValidity" value="false"/>
      <parameter id="ShowWarning" value="false"/>
